    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flavor: Staging",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_staging.dart",
            "args": [
                "--flavor",
                "staging",
                "--dart-define=env=stag"
