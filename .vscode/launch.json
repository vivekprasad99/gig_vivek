        {
            "name": "Flavor: Production",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main_production.dart",
            "args": [
                "--flavor",
                "production",
                "--dart-define=env=prod"
              ]
        }
    ]
}

